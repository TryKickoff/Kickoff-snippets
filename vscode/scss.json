{
	"scss-active": {
		"prefix": "active",
		"body": "&:active {\n\t$0\n}",
		"description": "Kickoff - :active"
	},
	"scss-after": {
		"prefix": "after",
		"body": "&::after {\n\tcontent: \'\';\n\t$0\n}",
		"description": "Kickoff - ::after"
	},
	"scss-before": {
		"prefix": "before",
		"body": "&::before {\n\tcontent: \'\';\n\t$0\n}",
		"description": "Kickoff - ::before"
	},
	"scss-before-after": {
		"prefix": "before",
		"body": "&::before,\n&::after {\n\tcontent: \'\';\n\t$0\n}",
		"description": "Kickoff - ::before/::after"
	},
	"scss-map-deep-get": {
		"prefix": "map-deep-get",
		"body": "ko-map-deep-get(${1:\$sass-map}, '${2:child}'${3:, '${4:child}'})",
		"description": "Kickoff - map-deep-get function"
	},
	"scss-modular-scale": {
		"prefix": "modular-scale",
		"body": "modular-scale(\$font-size-base, ${1:2}, \$type-scale)",
		"description": "Kickoff - modular-scale font-size function"
	},
	"scss-multiply": {
		"prefix": "multiply",
		"body": "ko-multiply(${1:\$baseline}, ${2:1})",
		"description": "Kickoff - multiply function"
	},
	"scss-hover": {
		"prefix": "hover",
		"body": "\&:hover {\n\t$0\n}",
		"description": "Kickoff - :hover"
	},
	"scss-link": {
		"prefix": "link",
		"body": "\&:link {\n\t$0\n}",
		"description": "Kickoff - :link"
	},
	"scss-visited": {
		"prefix": "visited",
		"body": "\&:visited {\n\t$0\n}",
		"description": "Kickoff - :visited"
	},
	"scss-font-size": {
		"prefix": "fs",
		"body": "@include ko-font-size(${1:16/mid/$font-large})$0",
		"description": "Kickoff - font-size() REM mixin"
	},
	"scss-hidpi": {
		"prefix": "hidpi",
		"body": "@include ko-hidpi(${1:1.5}){\n\t$0\n}",
		"description": "Kickoff - hidpi mixin"
	},
	"scss-media": {
		"prefix": "mq",
		"body": "@include media(${1:'>mid'}) {\n\t$0\n}",
		"description": "Kickoff - media() (Media Query) mixin"
	},
	"scss-transition": {
		"prefix": "transition",
		"body": "transition: ${1:\$default-transition};$0",
		"description": "Kickoff - transition (var)"
	},
	"scss-colors": {
		"prefix": "color",
		"body": "\$color-${1:primary|secondary|highlight|text|bg}",
		"description": "Kickoff - $color var"
	},
	"scss-font-family": {
		"prefix": "font-family",
		"body": "\$font-family-${1:base|sans|serif|mono}",
		"description": "Kickoff - $font-family var"
	},
	"scss-size": {
		"prefix": "size",
		"body": "@include ko-size(${1:10}px,${2:10}px);$0",
		"description": "Kickoff - size() mixin"
	},
	"scss-square": {
		"prefix": "square",
		"body": "@include ko-square(${1:50}px);$0",
		"description": "Kickoff - square() mixin"
	}
}
